{
  "name": "coupin-app",
  "main": "server.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.2.0"
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "body-parser": "~1.4.2",
    "cloudinary": "^1.9.0",
    "connect-busboy": "0.0.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.8",
    "dotenv": "^4.0.0",
    "express": "~4.8.0",
    "express-session": "^1.15.6",
    "express-validator": "^3.2.1",
    "fs-extra": "^3.0.1",
    "jsonwebtoken": "^8.0.1",
    "lodash": "^4.17.10",
    "mailgun-js": "^0.17.0",
    "method-override": "~2.0.2",
    "moment": "^2.19.1",
    "mongoose": "^4.9.10",
    "mongoose-seed": "^0.3.2",
    "morgan": "^1.9.0",
    "node-cron": "^1.2.1",
    "nodemailer": "^4.1.2",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-auth": "^1.0.2",
    "passport-http": "*",
    "passport-http-bearer": "*",
    "passport-jwt": "^3.0.0",
    "passport-local": "*",
    "passport-twitter": "^1.0.4",
    "raven": "^2.4.2",
    "shortid32": "^0.1.1"
  },
  "apidoc": {
    "name": "coupin-api-doc",
    "version": "0.0.1",
    "description": "coupin's api documentation",
    "title": "Coupin's Api Documentation",
    "url": "https://localhost:5030/api/v1",
    "sampleUrl": "https://localhost:5030/api/v1",
    "header": {
      "title": "Start",
      "filename": "header.md"
    },
    "footer": {
      "title": "End",
      "filename": "footer.md"
    },
    "template": {
      "withCompare": true,
      "withGenerator": true
    }
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-plugin-angular": "^3.3.0",
    "nodemon": "^1.12.1"
  },
  "scripts": {
    "preinstall": "npm install apidoc -g",
    "generate-docs": "apidoc -i app/ -o apiDoc/ -t apiDocTemplate/",
    "start": "npm run-script generate-docs && node init.js && nodemon server.js"
  }
}
